/dts-v1/;
/plugin/;

#include "../../rk3568-pinctrl.dtsi"

/ {
	fragment@0 {
		// i2c3 aliased with i2c0.
		// This activates i2c3 but it will be named as i2c0 on the userspace.
		target = <&i2c3>;

		__overlay__ {
			status = "okay";

			pinctrl-names = "default";
			pinctrl-0 = <&i2c3m1_xfer>;
		};
	};
};