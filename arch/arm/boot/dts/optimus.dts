/dts-v1/;

#include "ls1024a.dtsi"
/ {
	model = "Google Fiber Optimus";
	compatible = "google,optimus", "fsl,ls1024a";

	memory {
		device_type = "memory";
		reg = <0x80000000 0x40000000>; /* 1 GB */
	};

	refclk: oscillator {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <24000000>;
	};

};
